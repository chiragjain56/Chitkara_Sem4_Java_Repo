package com.chitkara.arrays;

import java.util.Arrays;
import java.util.Collections;

class Student {
	private int rollNo;
	private String name;

	public Student(int roll, String name) {
		this.name = name;
		this.rollNo = roll;
	}

	void print() {
		System.out.println("Roll no : " + rollNo + "\n" + "name: " + name);
	}
}

