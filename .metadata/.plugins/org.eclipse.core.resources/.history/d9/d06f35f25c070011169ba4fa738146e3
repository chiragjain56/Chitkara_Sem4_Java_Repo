package com.chitkara.arrays;

import java.util.Scanner;

class Student {
	private int id;
	private String name;

	public Student(int id, String name) {

		this.id = id;
		this.name = name;
	}

}

public class DemoG6 {
	public static void main(String[] args) {
		
	Student[] students = new Student[6];
	students[0] = new Student(1,"Harsh");
	students[1] = new Student(2, "Palak");
	students[2] = new Student(3,"Parul");
	students[3] = new Student(4, "Urvi");
	students[4] = new Student(5,"Vishnu");
	students[5] = new Student(6, "Devansh");
	
		
		
		
		
		
		
		
		
