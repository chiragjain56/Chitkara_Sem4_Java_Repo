package com.chitkara.interfac;

interface Shape1 {

	void getArea();

	void getPerimeter();
}

class Rectangle implements Shape1 {

	double area;
	double perimeter;

	public Rectangle(int l, int w) {
		this.area = l * w;
		this.perimeter = 2 * (l + w);
	}

	@Override
	public void getArea() {

	}

	@Override
	public void getPerimeter() {

	}

}

public class G9 {

}
